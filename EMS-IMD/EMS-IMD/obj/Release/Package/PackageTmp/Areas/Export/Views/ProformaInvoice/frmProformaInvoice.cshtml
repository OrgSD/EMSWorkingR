                <div class="col-xs-0 col-sm-6 col-md-3 col-lg-3">
                    <select id="PaymentType" name="PaymentType" class="RequiredField form-control">
                        <option value="Prepaid">Prepaid</option>
                        <option value="Collected">Collected</option>
                    </select>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">Approval Status:</div>
                <div class="col-xs-0 col-sm-6 col-md-3 col-lg-3">
                    <select id="ApprovedStatus" name="ApprovedStatus" class="RequiredField form-control">
                        <option value="">Not Approved</option>
                        <option value="Approved">Approved</option>
                        <option value="Cancel">Cancel</option>
                    </select>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="row">
                <div class="col-lg-3">Approval Remarks: </div>
                <div class="col-lg-9"><textarea id="ApprovedRemarks" name="ApprovedRemarks" placeholder="Approval Remarks" maxlength="50" class="form-control"></textarea></div>               
